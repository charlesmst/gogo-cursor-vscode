{
    "name": "gogo-cursor-savepoints",
    "displayName": "GoGo cursor savepoints",
    "description": "Save multiple cursor points and navigate between them",
    "version": "0.4.0",
    "publisher": "charlesmst",
    "issues":"https://github.com/charlesmst/gogo-cursor-vscode/issues",
    "engines": {
        "vscode": "^1.5.0"
    },
    "license": "MIT",
    "categories": [
        "Other"
    ],
    "homepage": "https://github.com/charlesmst/gogo-cursor-vscode",
    "repository": {
        "type": "git",
        "url": "https://github.com/charlesmst/gogo-cursor-vscode.git"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.gogo-cursor.clearAll",
                "title": "GoGo: Clear all savepoints"
            },
            {
                "command": "extension.gogo-cursor.showCurrent",
                "title": "GoGo: Show current savepoint"
            },
            {
                "command": "extension.gogo-cursor.saveAndNext",
                "title": "GoGo: Save to current and go next available savepoint"
            },
            {
                "command": "extension.gogo-cursor.goToPosition1",
                "title": "GoGo: go to cursor savepoint 1"
            },
            {
                "command": "extension.gogo-cursor.goToPosition2",
                "title": "GoGo: go to cursor savepoint 2"
            },
            {
                "command": "extension.gogo-cursor.goToPosition3",
                "title": "GoGo: go to cursor savepoint 3"
            },
            {
                "command": "extension.gogo-cursor.goToPosition4",
                "title": "GoGo: go to cursor savepoint 4"
            },
            {
                "command": "extension.gogo-cursor.goToPosition5",
                "title": "GoGo: go to cursor savepoint 5"
            },
            {
                "command": "extension.gogo-cursor.goToPosition6",
                "title": "GoGo: go to cursor savepoint 6"
            },
            {
                "command": "extension.gogo-cursor.goToPosition7",
                "title": "GoGo: go to cursor savepoint 7"
            },
            {
                "command": "extension.gogo-cursor.goToPosition8",
                "title": "GoGo: go to cursor savepoint 8"
            },
            {
                "command": "extension.gogo-cursor.goToPosition9",
                "title": "GoGo: go to cursor savepoint 9"
            }
        ],
        "keybindings": [
            {
                "command": "extension.gogo-cursor.saveAndNext",
                "key": "alt+shift+g"
            },
            {
                "command": "extension.gogo-cursor.goToPosition1",
                "key": "alt+shift+1"
            },
            {
                "command": "extension.gogo-cursor.goToPosition2",
                "key": "alt+shift+2"
            },
            {
                "command": "extension.gogo-cursor.goToPosition3",
                "key": "alt+shift+3"
            },
            {
                "command": "extension.gogo-cursor.goToPosition4",
                "key": "alt+shift+4"
            },
            {
                "command": "extension.gogo-cursor.goToPosition5",
                "key": "alt+shift+5"
            },
            {
                "command": "extension.gogo-cursor.goToPosition6",
                "key": "alt+shift+6"
            },
            {
                "command": "extension.gogo-cursor.goToPosition7",
                "key": "alt+shift+7"
            },
            {
                "command": "extension.gogo-cursor.goToPosition8",
                "key": "alt+shift+8"
            },
            {
                "command": "extension.gogo-cursor.goToPosition9",
                "key": "alt+shift+9"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}